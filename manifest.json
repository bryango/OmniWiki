{
  "name": "OmniWiki Extra",
  "version": "0.2",
  "manifest_version": 2,
  "description": "__MSG_extension_description__",
  "background": {
    "scripts": ["js/background.js"]
  },
  "omnibox": {
    "keyword": "w"
  },
  "permissions": [
    "https://*.wikipedia.org/*",
    "https://*.wikimedia.org/*"
  ],
  "icons": {
      "16": "img/16.png",
      "48": "img/48.png",
      "128": "img/128.png"
  },
  "minimum_chrome_version": "18",
  "options_page": "options.html",
  "default_locale": "en"
}
